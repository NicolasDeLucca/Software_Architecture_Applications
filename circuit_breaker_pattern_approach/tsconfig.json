{
    "compilerOptions": {
      "target": "ES2020",                   
      "module": "commonjs",             
      "outDir": "./dist",
      "rootDir": "./",                 
      "strict": true,                    
      "esModuleInterop": true,            
      "skipLibCheck": true,               
      "forceConsistentCasingInFileNames": true
    },
    "include": [
        "circuitBreakerService/*.ts",
        "externalService/*.ts"
    ],             
    "exclude": [
        "node_modules"
    ] 
}
 
